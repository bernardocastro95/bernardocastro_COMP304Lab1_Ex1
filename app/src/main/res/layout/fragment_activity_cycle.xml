<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityCycle"
    android:id="@+id/fragment2">


    <TextView
        android:id="@+id/activity_type"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/activity_type_size"/>

    <TextView
        android:id="@+id/onStart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/activity_type_size"
        android:layout_marginTop="@dimen/text_start_distance"/>
    <TextView
        android:id="@+id/onCreate"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/activity_type_size"
        android:layout_marginTop="@dimen/text_create_distance"/>

</FrameLayout>